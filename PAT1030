#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>


void sort(int *a,int n){//冒泡排序 
	int i,ex_number;
	bool flag=true;
	
	while(flag){
		flag=false;
		for(i=0;i<n-1;i++){
			if(a[i]>a[i+1]){
				ex_number=a[i];
				a[i]=a[i+1];
				a[i+1]=ex_number;
				flag=true;
			}
		}
	}
}

int judge(int *a,int m,int n){
	int i;
	for(i=0;i<m;i++){
		if(a[i]*m>a[n-1]){
			break;
		}
	}
	return i;
}


int main(void){
	int i,j,k;
	int n,m;
	int *a;
	
	scanf("%d%d",&n,&m);
	a=(int *)malloc(sizeof(int)*n);
	for(i=0;i<n;i++){
		scanf("%d",&(a[i]));
	}
	sort(a,n);
	printf("%d",n-judge(a,m,n));	
}
